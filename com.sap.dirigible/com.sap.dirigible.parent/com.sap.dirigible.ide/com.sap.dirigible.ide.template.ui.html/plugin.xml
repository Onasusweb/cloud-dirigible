<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:com.sap.dirigible.ide.workspace.ui.view.Menu.new?after=separator.custom">
         <command
               commandId="com.sap.dirigible.ide.template.ui.html.command.HtmlCommandHandler"
               icon="icons/wizard_user-interface.png"
               label="User Interface"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:com.sap.dirigible.ide.workspace.ui.view.Menu.new?after=separator.custom">
         <command
               commandId="com.sap.dirigible.ide.template.ui.html.command.HtmlCommandHandler"
               icon="icons/wizard_user-interface.png"
               label="User Interface"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.sap.dirigible.ide.workspace.ui.view.Menu.new?after=separator.custom">
         <command
               commandId="com.sap.dirigible.ide.template.ui.html.command.WikiCommandHandler"
               icon="icons/wizard_wiki.png"
               label="Wiki Page"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:com.sap.dirigible.ide.workspace.ui.view.Menu.new?after=separator.custom">
         <command
               commandId="com.sap.dirigible.ide.template.ui.html.command.WikiCommandHandler"
               icon="icons/wizard_wiki.png"
               label="Wiki Page"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.sap.dirigible.ide.workspace.ui.view.Menu.generate">
         <command
               commandId="com.sap.dirigible.ide.template.ui.html.command.HtmlForEntityCommandHandler"
               icon="icons/wizard_user-interface.png"
               label="User Interface for Entity Service"
               style="push">
               <visibleWhen
                     checkEnabled="false">
                  <and>
                     <count
                           value="1">
                     </count>
                     <iterate
                           ifEmpty="false"
                           operator="and">
                        <instanceof
                              value="org.eclipse.core.resources.IFile">
                        </instanceof>
                        <test
                              property="org.eclipse.core.resources.extension"
                              value="entity">
                        </test>
                     </iterate>
                  </and>
               </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="com.sap.dirigible.ide.template.ui.html.command.HtmlCommandHandler"
            id="com.sap.dirigible.ide.template.ui.html.command.HtmlCommandHandler"
            name="User Interface">
      </command>
      <command
            defaultHandler="com.sap.dirigible.ide.template.ui.html.command.WikiCommandHandler"
            id="com.sap.dirigible.ide.template.ui.html.command.WikiCommandHandler"
            name="Wiki Page">
      </command>
      <command
            defaultHandler="com.sap.dirigible.ide.template.ui.html.command.HtmlForEntityCommandHandler"
            id="com.sap.dirigible.ide.template.ui.html.command.HtmlForEntityCommandHandler"
            name="User Interface for Entity Service">
      </command>
   </extension>
